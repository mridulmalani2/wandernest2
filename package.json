{
  "name": "tourwiseco",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "preinstall": "node scripts/verify-dependency-allowlist.js",
    "postinstall": "prisma generate --schema=./src/prisma/schema.prisma",
    "vercel-build": "node scripts/check-db-url.js && prisma generate --schema=./src/prisma/schema.prisma && prisma migrate deploy --schema=./src/prisma/schema.prisma && next build",
    "db:reset": "echo '⚠️  DISABLED FOR SAFETY: This would delete ALL data. Use db:reset:dev ONLY against a local/dev database.' && exit 1",
    "db:reset:dev": "ALLOW_DB_RESET=true NODE_ENV=development node scripts/reset-neon-db.js && prisma migrate deploy --schema=./src/prisma/schema.prisma",
    "db:verify": "node scripts/verify-migration-status.js",
    "db:migrate": "prisma migrate deploy --schema=./src/prisma/schema.prisma",
    "db:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "test:api-exposure": "node scripts/api-exposure-smoke.js"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  },
