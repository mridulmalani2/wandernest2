# Modern browsers only - removes polyfills for Array.prototype.at, flat, flatMap, Object.fromEntries, etc.
# This targets browsers with ES2020+ support

[production]
# Target browsers from 2019 onwards with modern JS support
chrome >= 91
edge >= 91
firefox >= 90
safari >= 14.1
ios_saf >= 14.5
# Exclude IE completely
not IE 11
not dead

[development]
last 1 chrome version
last 1 firefox version
last 1 safari version
